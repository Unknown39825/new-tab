{
  "manifest_version": 2,
  
  "name": "New Tab page",
  "description": "Overrides the new tab page",
  "version": "1.3",
  
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' object-src 'self'",

  "author": "Chetan Verma"
}
